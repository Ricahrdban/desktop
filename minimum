public class Main {
    public static void main(String[] args) {
    int []arr = new int []{20,10,30,50,40};
    int min = arr [0];
      for (int i = 1; 1 < arr.length ; i++)
        {
         if(arr{i}>min)
           {
          min = arr[i];
        }
               }
          System.out.println("minimum value is" + min);
    }
}
